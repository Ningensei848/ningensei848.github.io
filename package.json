{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "ningensei-848-github-io",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "docusaurus": "docusaurus",
        "start": "pnpm emit && docusaurus start --port 3333",
        "build": "pnpm emit && docusaurus build",
        "swizzle": "docusaurus swizzle",
        "deploy": "docusaurus deploy",
        "clear": "docusaurus clear",
        "serve": "docusaurus serve",
        "write-translations": "docusaurus write-translations",
        "write-heading-ids": "docusaurus write-heading-ids",
        "typecheck": "tsc",
        "emit": "swc src/plugins -d . --config-file src/plugins/.swcrc",
        "dev": "pnpm start",
        "lint": "eslint",
        "lint:all": "eslint \"{src,tests}/**/*.{ts,tsx}\"",
        "lint-staged": "lint-staged",
        "format": "prettier --ignore-path .gitignore --write './**/*.{js,jsx,ts,tsx,json,css,md,mdx}' --ignore-unknown",
        "prepare": "husky"
    },
    "dependencies": {
        "@docusaurus/core": "3.9.2",
        "@docusaurus/plugin-content-blog": "^3.9.2",
        "@docusaurus/plugin-content-docs": "3.9.2",
        "@docusaurus/plugin-pwa": "3.9.2",
        "@docusaurus/preset-classic": "3.9.2",
        "@docusaurus/theme-common": "^3.9.2",
        "@mdx-js/react": "3.1.0",
        "@remark-embedder/core": "^3.0.3",
        "@remark-embedder/transformer-oembed": "^3.0.0",
        "@swc/cli": "0.5.0",
        "@swc/core": "1.15.3",
        "clsx": "^2.1.1",
        "next-share": "^0.27.0",
        "prism-react-renderer": "2.4.1",
        "react": "19.2.1",
        "react-dom": "19.2.1"
    },
    "devDependencies": {
        "@docusaurus/module-type-aliases": "3.9.2",
        "@docusaurus/tsconfig": "3.9.2",
        "@eslint/js": "^9.39.1",
        "@types/react": "19.2.1",
        "@types/react-dom": "19.2.1",
        "@typescript-eslint/eslint-plugin": "8.18.0",
        "@typescript-eslint/parser": "8.18.0",
        "dotenv": "^17.2.3",
        "eslint": "9.39.1",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-react": "7.37.2",
        "eslint-plugin-react-hooks": "5.1.0",
        "globals": "15.14.0",
        "husky": "9.1.7",
        "lint-staged": "16.2.6",
        "prettier": "3.7.3",
        "remark": "^15.0.1",
        "typescript": "~5.7.2",
        "typescript-eslint": "8.18.0"
    },
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true,
        "jsxSingleQuote": true,
        "embeddedLanguageFormatting": "off",
        "overrides": [
            {
                "files": "*.{md,mdx}",
                "options": {
                    "tabWidth": 2
                }
            }
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "pnpm lint"
        }
    },
    "lint-staged": {
        "*.@(ts|tsx)": [
            "pnpm lint --fix",
            "pnpm format --ignore-unknown"
        ],
        "!(*.ts|*.tsx)": [
            "pnpm format --ignore-unknown"
        ]
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": ">=22.12.0"
    }
}
