{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "ningensei-848-github-io",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "docusaurus": "docusaurus",
        "start": "yarn emit && docusaurus start --port 3333",
        "build": "yarn emit && docusaurus build",
        "swizzle": "docusaurus swizzle",
        "deploy": "docusaurus deploy",
        "clear": "docusaurus clear",
        "serve": "docusaurus serve",
        "write-translations": "docusaurus write-translations",
        "write-heading-ids": "docusaurus write-heading-ids",
        "typecheck": "tsc",
        "emit": "swc src/plugins -d src --config-file src/plugins/.swcrc",
        "dev": "yarn start",
        "postinstall": "node --eval 'process.exit(process.env.NODE_ENV === `production` ? 0 : 1)' || husky install",
        "lint": "eslint \"{src,tests}/**/*.{ts,tsx}\"",
        "lint-staged": "lint-staged",
        "format": "prettier --ignore-path .gitignore --write './**/*.{js,jsx,ts,tsx,json,css,md,mdx}'"
    },
    "dependencies": {
        "@docusaurus/core": "2.3.1",
        "@docusaurus/plugin-google-tag-manager": "2.3.1",
        "@docusaurus/plugin-pwa": "2.3.1",
        "@docusaurus/preset-classic": "2.3.1",
        "@mdx-js/react": "1.6.22",
        "@remark-embedder/core": "3.0.1",
        "@remark-embedder/transformer-oembed": "3.0.0",
        "@swc/cli": "0.1.60",
        "@swc/core": "1.3.31",
        "clsx": "1.2.1",
        "dotenv": "16.0.3",
        "next-share": "0.19.0",
        "prism-react-renderer": "1.3.5",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@docusaurus/module-type-aliases": "2.3.1",
        "@docusaurus/theme-common": "2.3.1",
        "@tsconfig/docusaurus": "1.0.6",
        "@typescript-eslint/eslint-plugin": "5.50.0",
        "@typescript-eslint/parser": "5.50.0",
        "eslint": "8.33.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "husky": "8.0.3",
        "lint-staged": "13.1.0",
        "prettier": "2.8.3",
        "remark": "14.0.2",
        "typescript": "4.9.5"
    },
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true,
        "jsxSingleQuote": true,
        "trailingComma": "all",
        "embeddedLanguageFormatting": "off"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "yarn lint"
        }
    },
    "lint-staged": {
        "*.@(ts|tsx)": [
            "yarn lint",
            "yarn format --ignore-unknown"
        ],
        "**/*": [
            "yarn format --ignore-unknown"
        ]
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": "16.x"
    }
}
