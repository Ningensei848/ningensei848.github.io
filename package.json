{
  "name": "laom",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "emit": "swc src/plugins -d src --config-file src/plugins/.swcrc",
    "docusaurus": "docusaurus",
    "dev": "yarn start",
    "start": "yarn emit && docusaurus start",
    "build": "yarn emit && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "typecheck": "tsc",
    "lint": "eslint --cache --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix && yarn format",
    "format": "prettier --ignore-path .gitignore --write './**/*.{js,jsx,ts,tsx,json,css,md,mdx}'",
    "update": "npx -p npm-check-updates  -c 'ncu -u'"
  },
  "dependencies": {
    "@cmfcmf/docusaurus-search-local": "^0.11.0",
    "@docusaurus/core": "^2.2.0",
    "@docusaurus/plugin-pwa": "^2.2.0",
    "@docusaurus/preset-classic": "^2.2.0",
    "@mdx-js/react": "^2.1.5",
    "@remark-embedder/core": "^3.0.1",
    "@remark-embedder/transformer-oembed": "^3.0.0",
    "clsx": "^1.2.1",
    "dotenv": "^16.0.3",
    "next-share": "^0.19.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.2.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.14",
    "@tsconfig/docusaurus": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "none",
    "embeddedLanguageFormatting": "off"
  },
  "eslintIgnore": [
    "node_modules",
    "build",
    ".docusaurus",
    ".eslintrc.js",
    "sidebars.js",
    "*.config.js"
  ],
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
