{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "ningensei-848-github-io",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "docusaurus": "docusaurus",
        "start": "npm run emit && docusaurus start --port 3333",
        "build": "npm run emit && docusaurus build",
        "swizzle": "docusaurus swizzle",
        "deploy": "docusaurus deploy",
        "clear": "docusaurus clear",
        "serve": "docusaurus serve",
        "write-translations": "docusaurus write-translations",
        "write-heading-ids": "docusaurus write-heading-ids",
        "typecheck": "tsc",
        "emit": "swc src/plugins -d src --config-file src/plugins/.swcrc",
        "dev": "npm run start",
        "postinstall": "node --eval 'process.exit(process.env.NODE_ENV === `production` ? 0 : 1)' || husky install",
        "lint": "eslint \"{src,tests}/**/*.{ts,tsx}\"",
        "lint-staged": "lint-staged",
        "format": "prettier --ignore-path .gitignore --write './**/*.{js,jsx,ts,tsx,json,css,md,mdx}' --ignore-unknown"
    },
    "dependencies": {
        "@docusaurus/core": "^3.0.0-alpha.0",
        "@docusaurus/plugin-content-docs": "^3.0.0-alpha.0",
        "@docusaurus/plugin-pwa": "^3.0.0-alpha.0",
        "@docusaurus/preset-classic": "^3.0.0-alpha.0",
        "@mdx-js/react": "^2.3.0",
        "@remark-embedder/core": "^3.0.1",
        "@remark-embedder/transformer-oembed": "^3.0.0",
        "@swc/cli": "^0.1.62",
        "@swc/core": "^1.3.67",
        "clsx": "^1.2.1",
        "next-share": "^0.20.0",
        "prism-react-renderer": "^1.3.5",
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@docusaurus/module-type-aliases": "^3.0.0-alpha.0",
        "@docusaurus/tsconfig": "^3.0.0-alpha.0",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "dotenv": "^16.3.1",
        "eslint": "^8.43.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eta": "^3.0.3",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.3",
        "prettier": "^2.8.8",
        "remark": "^14.0.3",
        "typescript": "^5.1.5"
    },
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true,
        "jsxSingleQuote": true,
        "trailingComma": "all",
        "embeddedLanguageFormatting": "off"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run lint"
        }
    },
    "lint-staged": {
        "*.@(ts|tsx)": [
            "npm run lint",
            "npm run format --ignore-unknown"
        ],
        "**/*": [
            "npm run format --ignore-unknown"
        ]
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": "18.x"
    }
}
