{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "ningensei-848-github-io",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "docusaurus": "docusaurus",
        "start": "npm run emit && docusaurus start --port 3333",
        "build": "npm run emit && docusaurus build",
        "swizzle": "docusaurus swizzle",
        "deploy": "docusaurus deploy",
        "clear": "docusaurus clear",
        "serve": "docusaurus serve",
        "write-translations": "docusaurus write-translations",
        "write-heading-ids": "docusaurus write-heading-ids",
        "typecheck": "tsc",
        "emit": "swc src/plugins -d . --config-file src/plugins/.swcrc",
        "dev": "npm run start",
        "lint": "eslint \"{src,tests}/**/*.{ts,tsx}\"",
        "lint-staged": "lint-staged",
        "format": "prettier --ignore-path .gitignore --write './**/*.{js,jsx,ts,tsx,json,css,md,mdx}' --ignore-unknown",
        "prepare": "husky"
    },
    "dependencies": {
        "@docusaurus/core": "^3.2.1",
        "@docusaurus/plugin-content-docs": "^3.2.1",
        "@docusaurus/plugin-pwa": "^3.2.1",
        "@docusaurus/preset-classic": "^3.2.1",
        "@mdx-js/react": "^3.0.1",
        "@remark-embedder/core": "^3.0.2",
        "@remark-embedder/transformer-oembed": "^3.0.0",
        "@swc/cli": "^0.3.12",
        "@swc/core": "^1.4.16",
        "clsx": "^2.1.0",
        "next-share": "^0.27.0",
        "prism-react-renderer": "^2.3.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@docusaurus/module-type-aliases": "^3.2.1",
        "@docusaurus/tsconfig": "^3.2.1",
        "@types/react": "^18.2.79",
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@typescript-eslint/parser": "^7.7.0",
        "dotenv": "^16.4.5",
        "eslint": "^9.1.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "remark": "^15.0.1",
        "typescript": "^5.4.5"
    },
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true,
        "jsxSingleQuote": true,
        "embeddedLanguageFormatting": "off"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run lint"
        }
    },
    "lint-staged": {
        "*.@(ts|tsx)": [
            "npm run lint",
            "npm run format --ignore-unknown"
        ],
        "**/*": [
            "npm run format --ignore-unknown"
        ]
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": ">=18.0"
    }
}
